{
    "extends": "airbnb-base",
    "rules": {
        "no-plusplus": "off",
        "one-var": "off",
        "one-var-declaration-per-line": "off",
        "max-len": "off",
        "no-param-reassign": [
            "error",
            {
                "props": false
            }
        ],
        "no-continue": "off"
    }
}
